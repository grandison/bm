<% if @with_order %>
  window.location = '<%= url_for(@order) %>';
<% else %>
  <% if @count %>
    $("#emails-count").text("<%= @count %> емейлов найдено.");
    $("#new_order :input").removeAttr("disabled");
  <% else %>
  setTimeout(function(){
    $("#new_order :input").removeAttr("disabled");
    submit_form();
  }, 10000);
  <% end %>
<% end %>